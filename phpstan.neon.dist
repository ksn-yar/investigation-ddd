includes:
    - vendor/phpstan/phpstan-symfony/extension.neon
    - vendor/phpstan/phpstan-symfony/rules.neon
parameters:
    level: 9
    paths:
        - config/
        - src/
        - tests/
    tmpDir: ./var/cache/.phpstan-cache
    treatPhpDocTypesAsCertain: false
    excludePaths:
        - vendor
        - var
